{
  "name": "vidlyzerbot",
  "version": "1.0.0",
  "description": "Discord bot dengan struktur modular",
  "main": "start.js",
  "scripts": {
    "start": "node start.js",
    "bot": "node src/index.js",
    "dev": "nodemon src/index.js",
    "deploy": "node deploy/deploy-unified.js",
    "deploy:global": "node deploy/deploy-unified.js global",
    "deploy:guild": "node deploy/deploy-unified.js guild",
    "deploy:simple": "node deploy/deploy-unified.js simple",
    "clear:global": "node deploy/deploy-unified.js clear-global",
    "clear:guild": "node deploy/deploy-unified.js clear-guild",
    "clear:all": "node deploy/deploy-unified.js clear-all",
    "auto-deploy": "node scripts/auto-deploy-standalone.js",
    "auto-deploy:guild": "node scripts/auto-deploy-standalone.js guild",
    "auto-deploy:global": "node scripts/auto-deploy-standalone.js global",
    "pterodactyl:simple": "node pterodactyl/start-simple.js",
    "pterodactyl:advanced": "node pterodactyl/start-advanced.js"
  },
  "keywords": [
    "discord",
    "bot",
    "discord.js"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^14.19.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}